package asds;

import java.util.stream.Stream;

public class StreamFibonacci {
	
	// The Fibonacci series is the sequence of numbers,
	// where every number is the sum of the preceding two numbers.

	public static void main(String[] args) {
	
		//Stream.iterate(new int[] {0,1},f->new int[] {f[1],f[0]+f[1]}).limit(10).map(f->f[0]).forEach(System.out::println);
		
		Integer nthFibo = Stream.iterate(new int[] {0,1},f->new int[] {f[1],f[0]+f[1]}).
				limit(10).map(f->f[0]).reduce((first,second)->second)
				.orElseThrow(() -> new IllegalStateException("No value present"));
				
		
			System.out.println(nthFibo);
		
		// Ensure println is correctly spelled
	}
}
